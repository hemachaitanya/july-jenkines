pipeline{
    agent{ label 'EEE'}
    tools {
        jdk 'JDK_8'
    }
    stages{
        stage ('vcs'){
            steps {
                git url: 'https://github.com/hemachaitanya/game-of-life.git',
                    branch: 'master'
            }
        }
        stage('build'){
            steps{
                sh 'java -version'
                sh 'mvn clean package'
            }
        }
    }
}