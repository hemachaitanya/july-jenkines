pipeline {
    agent { label 'EEE'}
    triggers {
        pollSCM('* * * * *')
    }
     stages {
        stage('vcs'){
            steps {
                git url: 'https://github.com/hemachaitanya/spring-petclinic.git',
                    branch: 'main'
            }
        }
        stage(build){
            steps{
                sh 'mvn clean package'
            }   
        }
       stage('sonarqube'){
        steps{
             withSonarQubeEnv('My SonarQube Server') {
                sh 'mvn clean package sonar:sonar'
            }
        }
       }
    }
}



   
